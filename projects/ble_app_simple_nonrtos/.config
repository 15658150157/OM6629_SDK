
#
# System
#
CONFIG_TOOL_CHAIN_OPTIMIZATION_LEVEL=4
# end of System

#
# BSP
#
# CONFIG_BOARD_NONE is not set
CONFIG_BOARD_EVB=y
CONFIG_GLCD_NONE=y
# CONFIG_GLCD_GC9C01 is not set
# CONFIG_GLCD_GC9B71 is not set
# end of BSP

#
# Components
#
# CONFIG_CHERRYUSB is not set
CONFIG_BLE=y
CONFIG_BLE_CONTROLLER=y
CONFIG_BLE_CONTROLLER_LIB_1PERIPHERAL=y
# CONFIG_BLE_CONTROLLER_LIB_FULL is not set
# CONFIG_BLE_CONTROLLER_LIB_FULL_PLUS is not set
# CONFIG_BLE_CONTROLLER_LIB_VELA_MESH is not set
CONFIG_BLE_HOST=y
CONFIG_BLE_PROFILE=y
# CONFIG_LE_LL_SEC_CON is not set
CONFIG_LE_HOST_ATT_MTU=247
CONFIG_LE_HOST_ATT_WRITE_CACHE_SIZE=23
CONFIG_LE_HOST_MAX_GATT_SERV_NUM=8
# CONFIG_LE_HOST_SC_PAIRING is not set
CONFIG_LE_HOST_ADV_SET_NUM=4
CONFIG_LE_HOST_CONNECTION_NB=1
CONFIG_LE_HOST_MSG_SIZE=512
CONFIG_EVT=y
CONFIG_FAULT_HANDLE=y
CONFIG_NVDS=y
CONFIG_OM_LOG=n
# CONFIG_OM_LOG_TO_SHELL is not set
# CONFIG_OM_LOG_TO_RTT is not set
CONFIG_OM_LOG_TO_UART=y
# CONFIG_OM_LOG_TO_FLASH is not set
CONFIG_OM_LOG_UART_IDX=1
CONFIG_OM_LOG_UART_BAUDRATE=115200
# CONFIG_OM_LOG_TIME is not set
CONFIG_OM_LOG_FILTER_LEVEL=5
CONFIG_OM_LOG_LINE_MAX=256
CONFIG_PM=y
CONFIG_PM_CHECKER_NUM=4
CONFIG_PM_STORE_CALLBACK_NUM=6
# CONFIG_SHELL is not set
# CONFIG_SLE is not set
# CONFIG_TRACE is not set
# end of Components

#
# Hal
#
# CONFIG_CMSIS_DSP is not set
# CONFIG_CMSIS_NN is not set
# end of Hal

#
# RTOS
#
CONFIG_NON_RTOS=y
# CONFIG_FREE_RTOS is not set
# end of RTOS

#
# Project
#
# CONFIG_XIP_RAM is not set
CONFIG_XIP_FLASH=y
# CONFIG_XIP_ROM is not set
CONFIG_OM_ASSERT=y
# end of Project
